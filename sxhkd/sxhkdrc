XF86MonBrightness{Down,Up}
	brightnessctl s 10%{-,+}

XF86AudioRaiseVolume
	pactl set-sink-volume @DEFAULT_SINK@ +5%

XF86AudioLowerVolume
	pactl set-sink-volume @DEFAULT_SINK@ -5%

XF86AudioMute
	pactl set-sink-mute @DEFAULT_SINK@ toggle 

alt + r
	rofi -show run

alt + shift + r
	rofi -show drun

super + f
	firefox

super + q
	qutebrowser

super + b
	brave

alt + shift + f
	st -e ranger

